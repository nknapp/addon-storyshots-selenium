{
	"compilerOptions": {
		"types": ["webpack-env", "node", "jest"],
		"declaration": true,
		"baseUrl": ".",
		"incremental": false,
		"noImplicitAny": false,
		"experimentalDecorators": true,
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"jsx": "react",
		"module": "commonjs",
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"target": "ES5",
		"lib": ["es2020", "dom"],
		"outDir": "dist"
	}
}
