{
  "name": "@knappi/addon-storyshots-selenium",
  "version": "0.0.0",
  "description": "Use storyshots via selenium",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "private": false,
  "funding": [
    {
      "type": "paypal",
      "url": "https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=GB656ZSAEQEXN&source=url"
    },
    {
      "type": "liberapay",
      "url": "https://liberapay.com/nils.knappmeier/donate"
    }
  ],
  "files": [
    "dist",
    "src/**/*.js",
    "src/**/*.json"
  ],
  "scripts": {
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:coverage --",
    "posttest": "npm run format",
    "format": "prettier --loglevel warn --write \"**/*.{js,ts,d.ts,css,md}\"",
    "lint": "eslint . --cache --fix",
    "check:format": "prettier --check \"**/*.{js,ts,d.ts,css,md}\"",
    "check:lint": "eslint . --cache --max-warnings 0",
    "thought": "thought run -a",
    "changelog": "standard-changelog && git add CHANGELOG.md",
    "version": "npm run changelog && npm run thought",
    "storybook": "start-storybook -p 9009",
    "build": "rimraf dist && babel src -s -d dist --ignore src/*.test.js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "browserstack-local": "^1.4.5",
    "child-service": "^2.0.2",
    "chisel-tunnel": "^1.0.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.7.0",
    "husky": ">=4",
    "jest": "25",
    "lint-staged": ">=10",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "standard-changelog": "^2.0.24",
    "thought": "4",
    "thought-plugin-jsdoc": "^2.0.0",
    "trace": "^3.1.1"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "jest-image-snapshot": "^4.0.2",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "webdriver": "^6.1.17"
  },
  "peerDependencies": {
    "@storybook/addon-storyshots": "^5.3.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,ts,d.ts,css,md}": "prettier --write"
  },
  "jest": {
    "rootDir": ".",
    "testMatch": [
      "**/src/**/*.test.js",
      "**/integration-test/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "author": "Nils Knappmeier <npm@knappi.org>",
  "repository": "git@github.com:nknapp/addons-storyshots-selenium.git"
}
